$('#booking-management-tbody').find('#booked_environment_<%= @booking.id %>').remove();
$('#errors').addClass('d-none');

$('#environment_<%= @environment.id %>').html('<%= @environment.status_message %>');

<% unless @environment.booked_environments.present? %>
  $('#modal-body').find('table').addClass('d-none');
  $('#modal-body').find('.be-the-first-msg').removeClass('d-none');
<% end %>
